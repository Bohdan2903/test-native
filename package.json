{
  "name": "reactNativePet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "watchman": "watchman watch-del-all",
    "metro-cache": "react-native start --reset-cache",
    "doctor": "npx react-native doctor",
    "info": "npx react-native info",
    "pods": "cd ios && pod install",
    "pods-update": "cd ios && pod deintegrate && pod update",
    "postinstall": "react-native setup-ios-permissions && react-native-asset",
    "prettier": "prettier --write ./src --loglevel silent",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --cache --ext .ts,.tsx --fix",
    "format": "npm run prettier && npm run lint:fix",
    "start": "react-native start",
    "test": "jest"
  },
  "reactNativePermissionsIOS": [
    "AppTrackingTransparency",
    "BluetoothPeripheral",
    "Calendars",
    "Camera",
    "Contacts",
    "FaceID",
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse",
    "MediaLibrary",
    "Microphone",
    "Motion",
    "Notifications",
    "PhotoLibrary",
    "PhotoLibraryAddOnly",
    "Reminders",
    "Siri",
    "SpeechRecognition",
    "StoreKit"
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.2",
    "@react-native/metro-config": "^0.72.11",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.6.1",
    "axios": "^1.4.0",
    "npm": "^9.8.1",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-native": "0.72.4",
    "react-native-asset": "^2.1.1",
    "react-native-date-picker": "^4.2.13",
    "react-native-fix-image": "^2.1.0",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-image-picker": "^5.6.0",
    "react-native-permissions": "^3.8.4",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.20.0",
    "react-native-snackbar": "^2.6.2",
    "react-native-svg": "^13.9.0",
    "react-native-touch-id": "^4.4.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-webview": "^13.3.1",
    "react-redux": "^8.0.7",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "7.14.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.8",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.1.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
